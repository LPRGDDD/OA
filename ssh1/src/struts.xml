<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<constant value="utf-8" name="struts.i18n.encoding"/>
	<package name="gd" extends="struts-default" namespace="/">
		<action name="person_*" class="personAction" method="{1}">
			<result name="find">/success.jsp</result>
			<result name="del">/success.jsp</result>
			<result name="get">/get.jsp</result>
			<result name="update">/success.jsp</result>
			<result name="save">/success.jsp</result>
			<result name="findByPages">/findByPages.jsp</result>
		</action>
		<action name="stu_*" class="studentAction" method="{1}">
			<result name="stu_find">/stu_find.jsp</result>
		</action>
		<action name="order_*" class="orderAction" method="{1}">
			<result name="queryOrder">/queryOrder.jsp</result>
			<result name="queryItem">/queryItem.jsp</result>
			<result name="get">/get.jsp</result>
			<result name="update">/queryOrder.jsp</result>
			<result name="getItem">/getItem.jsp</result>
			<result name="updateItem">/queryOrder.jsp</result>
		</action>
	</package>
</struts>    
